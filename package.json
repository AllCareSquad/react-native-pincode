{
  "name": "@haskkor/react-native-pincode",
  "version": "1.22.6",
  "description": "React native PIN code component",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "yarn build -watch",
    "build": "tsc",
    "compile": "rm -rf dist && npm run build",
    "addlocal": "yarn add file:./local_modules/react-native-pincode",
    "test": "jest",
    "pretty": "prettier --single-quote --no-semi --jsx-bracket-same-line --write \"src/**/*.{ts, tsx}\""
  },
  "jest": {
    "preset": "react-native",
    "cacheDirectory": "./cache",
    "transformIgnorePatterns": [
      "/node_modules/(?!react-native|@react-native|@react-native-async-storage)"
    ],
    "setupFiles": [
      "./jestSetupFile.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Haskkor/react-native-pincode.git"
  },
  "author": "Jeremy Farnault",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Haskkor/react-native-pincode/issues"
  },
  "homepage": "https://github.com/Haskkor/react-native-pincode#readme",
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@types/d3-ease": "^2.0.0",
    "@types/lodash": "^4.14.186",
    "@types/react": "^17.0.2",
    "@types/react-native": "^0.67.4",
    "babel": "^6.23.0",
    "babel-jest": "^29",
    "enzyme": "^3.11.0",
    "jest": "^29",
    "jest-serializer-enzyme": "^1.0.0",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.7.1",
    "react": "^17.0.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^17",
    "react-native": "^0.67.4",
    "react-test-renderer": "^17",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "d3-ease": "^2.0.0",
    "lodash": "^4.17.21",
    "react-move": "^6.5.0",
    "react-native-easy-grid": "^0.2.2",
    "react-native-keychain": "^8.1.1",
    "react-native-touch-id": "^4.4.1",
    "react-native-vector-icons": "^9.2.0"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "react": "*",
    "react-native": "*"
  }
}
